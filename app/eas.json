{
  "cli": {
    "version": ">= 3.8.1"
  },
  "build": {
    "development": {
      "distribution": "internal",
      "android": {
        "gradleCommand": ":app:assembleDebug"
      },
      "ios": {
        "simulator": true,
        "buildConfiguration": "Debug",
        "resourceClass": "m-medium"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium"
      }
    },
    "production": {
      "ios": {
        "resourceClass": "m-medium"
      },
      "env": {
          "ANDROID_SDK_ROOT": "C:/Users/ABHI/AppData/Local/Android/Sdk"
      }
      
    }
  },
  "submit": {
    "production": {}
  }
}
